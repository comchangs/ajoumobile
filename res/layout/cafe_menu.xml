<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
	<ScrollView android:layout_width="match_parent"
		android:layout_height="wrap_content" android:id="@+id/scrollView1">
		<LinearLayout android:layout_width="match_parent"
			android:id="@+id/linearLayout1" android:layout_height="match_parent"
			android:orientation="vertical" android:padding="5dp">
			<TextView android:text="@+id/TextView01" android:id="@+id/time1"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="18sp" android:textStyle="bold" android:textColor="#333"></TextView>
			<TextView android:text="@+id/TextView02" android:id="@+id/menu1"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="17sp" android:textColor="#666" android:layout_marginLeft="10sp"></TextView>
			<TextView android:text="@+id/TextView03" android:id="@+id/time2"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="18sp" android:textStyle="bold" android:textColor="#333"></TextView>
			<TextView android:text="@+id/TextView04" android:id="@+id/menu2"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="17sp" android:textColor="#666" android:layout_marginLeft="10sp"></TextView>
			<TextView android:text="@+id/TextView05" android:id="@+id/time3"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="18sp" android:textStyle="bold" android:textColor="#333"></TextView>
			<TextView android:text="@+id/TextView06" android:id="@+id/menu3"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="17sp" android:textColor="#666" android:layout_marginLeft="10sp"></TextView>
			<TextView android:text="@+id/TextView07" android:id="@+id/time4"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="18sp" android:textStyle="bold" android:textColor="#333"></TextView>
			<TextView android:text="@+id/TextView08" android:id="@+id/menu4"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:textSize="17sp" android:textColor="#666" android:layout_marginLeft="10sp"></TextView>

		</LinearLayout>
	</ScrollView>
</LinearLayout>
