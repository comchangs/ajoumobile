<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01" 
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent" 
	android:orientation="vertical"
	xmlns:android="http://schemas.android.com/apk/res/android" android:background="@drawable/background">
	<ImageView android:src="@drawable/titlebar" android:id="@+id/imageView1"
		android:layout_height="wrap_content" android:layout_width="wrap_content"></ImageView>
	<TextView android:id="@+id/textView1" android:text="열람실 좌석 조회 및 연장"
		android:padding="10sp" android:textSize="20sp" android:textColor="#333333"
		android:layout_width="fill_parent" android:textStyle="bold|italic"
		android:background="#661358be" android:layout_height="50sp"></TextView>
	<LinearLayout android:id="@+id/linearLayout1" android:layout_width="match_parent" android:orientation="vertical" android:layout_weight="1" android:layout_height="match_parent">
		<TextView android:layout_height="wrap_content" android:text="열람실 발급 내역(좌석)" android:id="@+id/readingroomextend_tv01" android:layout_gravity="center_vertical|center" android:layout_width="wrap_content" android:textColor="#333" android:layout_marginTop="70sp" android:textSize="22sp"></TextView>
		<TextView android:layout_height="wrap_content" android:text="열람실 발급 내역(시간)" android:id="@+id/readingroomextend_tv02" android:layout_gravity="center_vertical|center" android:layout_width="wrap_content" android:textSize="16sp" android:textColor="#333" android:layout_marginTop="20sp" android:layout_marginBottom="100sp"></TextView>
		<Button android:id="@+id/readingroomextend_btn01" android:layout_gravity="center_vertical|center" android:drawableTop="@drawable/plus_icon" android:background="#00000000" android:layout_height="wrap_content" android:layout_width="wrap_content"></Button>
	</LinearLayout>
</LinearLayout>	
